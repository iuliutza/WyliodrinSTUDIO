		"eslint-plugin-vue": "^5.2.3",
		"file-loader": "^4.0.0",
		"highcharts": "^7.1.2",
		"highcharts-vue": "^1.3.5",
		"highlight.js": "^9.15.8",
		"html-webpack-plugin": "^3.2.0",
		"jquery": "^3.4.1",
		"jszip": "^3.2.2",
		"katex": "^0.10.2",
		"less": "^3.9.0",
		"less-loader": "^5.0.0",
		"marked": "git+https://github.com/wyliodrinstudio/marked.git",
		"material-design-icons-iconfont": "^4.0.5",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.26",
		"monaco-editor": "^0.19.0",
		"monaco-editor-webpack-plugin": "^1.8.0",
		"msgpack5": "^4.2.1",
		"node-blockly": "https://github.com/wyliodrinstudio/node-blockly.git",
		"raw-loader": "^3.1.0",
		"reconnectingwebsocket": "^1.0.0",
		"request": "^2.88.0",
		"style-loader": "^1.0.0",
		"uuid": "^3.3.2",
		"validator": "^10.11.0",
		"vue": "^2.6.10",
		"vue-async-computed": "^3.7.0",
		"vue-chartjs": "^3.4.2",
		"vue-i18n": "^8.8.0",
		"vue-loader": "^15.7.0",
		"vue-monaco": "^1.1.0",
		"vue-multipane": "^0.9.5",
		"vue-resource": "^1.5.1",
		"vue-router": "^3.0.2",
		"vue-swatches": "^1.0.3",
		"vue-template-compiler": "^2.6.10",
		"vue2-ace-editor": "0.0.13",
		"vue2-highcharts": "^1.2.5",
		"vuedl": "^0.3.4",
		"vuetify": "2.0.3",
		"vuetify-dialog": "^0.3.6",
		"vuex": "^3.1.0",
