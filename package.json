	"eslintConfig": {
		"env": {
			"es6": true,
			"node": true,
			"browser": true,
			"mocha": false
		},
		"globals": {
			"node": true
		},
		"parserOptions": {
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"plugins": [
			"vue"
		],
		"extends": [
			"eslint:recommended",
			"plugin:vue/essential"
		],
		"rules": {
			"indent": [
				"error",
				"tab",
				{
					"SwitchCase": 1
				}
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"no-console": [
				"warn"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"require-await": [
				"error"
			]
		}
	},
	"dependencies": {
		"aedes": "^0.39.0",
		"architect": "^0.1.13",
		"archiver": "^3.0.0",
		"bonjour-hap": "^3.5.4",
		"cowsay": "^1.4.0",
		"desktop-screenshot": "^0.1.1",
		"drivelist": "^8.0.6",
		"electron-is-dev": "^1.1.0",
		"electron-log": "^3.0.9",
		"electron-updater": "^4.2.0",
		"fs-extra": "^7.0.1",
		"grabity": "^1.0.5",
		"irc-upd": "^0.10.0",
		"lodash": "^4.17.15",
		"material-design-icons": "^3.0.1",
		"material-icons": "^0.3.1",
		"node-notifier": "^5.4.0",
		"path": "^0.12.7",
		"request-progress": "^3.0.0",
		"screenshot-desktop": "^1.8.0",
		"serialport": "^8.0.8",
		"ssh2": "^0.8.2",
		"tree-kill": "^1.2.1",
		"unzipper": "^0.10.5",
		"vue-filereader": "^0.1.1",
		"zlib": "^1.0.5"
	}
}
